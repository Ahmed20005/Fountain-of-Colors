function Initialize()
  SKIN:Bang("!WriteKeyValue", "AudioNUM", "BandIdx", "NUM", "#ROOTCONFIGPATH#Template")
  
  if SKIN:GetVariable("MinBarHeight") ~= "0" and true or (SKIN:GetVariable("ModeKeyboard") ~= "0" and true or false) then

    SKIN:Bang("!WriteKeyValue", "NUM", "MeasureName", "AudioCalcNUM", "#ROOTCONFIGPATH#Template")
    SKIN:Bang("!WriteKeyValue", "Include", "@Include5", "CalcMeasures", "#ROOTCONFIGPATH##SkinGroup#")
  
    if SKIN:GetVariable("ModeKeyboard") ~= "0" then
      SKIN:Bang("!WriteKeyValue", "AudioCalcNUM", "Formula", "'(Max(AudioNUM, 1))'", "#ROOTCONFIGPATH#TemplateCalcMeasures")
    else
      SKIN:Bang("!WriteKeyValue", "AudioCalcNUM", "Formula",
        "'(Max(" .. 0.00001 + (SKIN:GetVariable("MinBarHeight") / SKIN:GetVariable("BarHeight"))
        .. ", AudioNUM))'", "#ROOTCONFIGPATH#TemplateCalcMeasures")
    end

  else
    SKIN:Bang("!DisableMeasure", "GenerateCalcMeasures")
    SKIN:Bang("!WriteKeyValue", "NUM", "MeasureName", "AudioNUM", "#ROOTCONFIGPATH#Template")
    SKIN:Bang("!WriteKeyValue", "Include", "@Include5", "", "#ROOTCONFIGPATH##SkinGroup#")
  end
end